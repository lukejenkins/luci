{
	"luci-app-range": {
		"description": "Grant access for LuCI wireless range test application",
		"read": {
			"ubus": {
				"luci.range": [
					"get_station_info",
					"start_test",
					"stop_test",
					"get_test_status"
				],
				"hostapd.*": [
					"get_clients",
					"get_status"
				],
				"network.wireless": [
					"status",
					"info"
				],
				"iwinfo": [
					"info",
					"assoclist",
					"freqlist",
					"txpowerlist",
					"countrylist"
				]
			},
			"file": {
				"/usr/bin/iw": [ "exec" ],
				"/sbin/iwinfo": [ "exec" ]
			}
		},
		"write": {
			"ubus": {
				"luci.range": [
					"start_test",
					"stop_test"
				]
			}
		}
	}
}
